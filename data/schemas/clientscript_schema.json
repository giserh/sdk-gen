{
   "$schema":"http://json-schema.org/draft-03/schema",
   "description":"A representation of a client script",
   "type":"object",
   "properties":{
      "name":{
         "required":true,
         "type":"string"
      },
      "code":{
         "required":true,
         "type":"string"
      },
      "returnType":{
         "required":true,
         "type":"string",
         "allowed":{
            "0":"NUMBER",
            "1":"STRING",
            "2":"BOOLEAN"
         },
         "default":"STRING"
      },
      "hash":{
         "required":false,
         "type":"string"
      }
   }
}