{
    "$schema": "http://json-schema.org/draft-03/schema",
    "type": "object",
    "description": "A representation of a client script",
    "properties": {
        "returnType": {
            "default": "STRING",
            "required": true,
            "type": "string",
            "description": "return value type",
            "allowed": {
                "1": "STRING",
                "0": "NUMBER",
                "2": "BOOLEAN"
            }
        },
        "code": {
            "required": true,
            "type": "string",
            "description": ""
        },
        "hash": {
            "required": false,
            "type": "string",
            "description": ""
        },
        "name": {
            "required": true,
            "type": "string",
            "description": "name used to describe the resource"
        }
    }
}