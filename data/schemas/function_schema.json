{
   "$schema":"http://json-schema.org/draft-03/schema",
   "description":"A representation of a counter",
   "type":"object",
   "properties":{
      "name":{
         "required":false,
         "type":"string"
      },
      "description":{
         "required":false,
         "type":"string"
      },
      "className":{
         "required":false,
         "type":"string"
      },
      "method":{
         "required":false,
         "type":"string"
      },
      "returnType":{
         "required":false,
         "type":"string",
         "allowed":{
            "0":"BOOLEAN",
            "1":"COMPARE"
         },
         "default":"BOOLEAN"
      },
      "functionType":{
         "required":false,
         "type":"string",
         "allowed":{
            "0":"NATIVE",
            "1":"SCRIPT"
         },
         "default":"NATIVE"
      },
      "scriptType":{
         "required":false,
         "type":"string",
         "allowed":{
            "0":"SYSTEM",
            "1":"CLIENT"
         },
         "default":"CLIENT"
      },
      "scriptId":{
         "required":false,
         "type":"long",
         "default":"0"
      },
      "manifesto":{
         "required":false,
         "type":"string"
      }
   }
}