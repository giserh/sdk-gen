{
    "$schema": "http://json-schema.org/draft-03/schema",
    "type": "object",
    "description": "A representation of a counter",
    "properties": {
        "scriptType": {
            "default": "CLIENT",
            "required": false,
            "type": "string",
            "description": "type of the script",
            "allowed": {
                "1": "CLIENT",
                "0": "SYSTEM"
            }
        },
        "name": {
            "required": false,
            "type": "string",
            "description": "name used to describe the resource"
        },
        "className": {
            "required": false,
            "type": "string",
            "description": "name of the class"
        },
        "scriptId": {
            "default": "0",
            "required": false,
            "type": "long",
            "description": "id of the script"
        },
        "manifesto": {
            "required": false,
            "type": "string",
            "description": "id of the type"
        },
        "returnType": {
            "default": "BOOLEAN",
            "required": false,
            "type": "string",
            "description": "return value type",
            "allowed": {
                "1": "COMPARE",
                "0": "BOOLEAN"
            }
        },
        "functionType": {
            "default": "NATIVE",
            "required": false,
            "type": "string",
            "description": "type of the function",
            "allowed": {
                "1": "SCRIPT",
                "0": "NATIVE"
            }
        },
        "method": {
            "required": false,
            "type": "string",
            "description": ""
        },
        "description": {
            "required": false,
            "type": "string",
            "description": "string containing the entity's description"
        }
    }
}