{
    "$schema": "http://json-schema.org/draft-03/schema",
    "type": "object",
    "description": "A representation of a transaction source",
    "properties": {
        "conditions": {
            "required": false,
            "type": "id_array",
            "description": "id of the connected conditions"
        },
        "name": {
            "required": false,
            "type": "string",
            "description": "name used to describe the resource"
        },
        "transactionSource": {
            "required": false,
            "type": "long",
            "description": "source of the current transaction"
        }
    }
}